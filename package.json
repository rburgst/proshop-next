{
  "name": "proshop-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn run next dev",
    "debug": "NODE_OPTIONS='--inspect' yarn run next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix",
    "autofix": "eslint --ext js,jsx,ts,tsx --fix .",
    "format": "prettier --write 'src/**/*.ts'",
    "data:import": "ts-node -O '{\"module\":\"CommonJs\"}' server/seeder",
    "data:destroy": "ts-node -O '{\"module\":\"CommonJs\"}' server/seeder -d"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "git-precommit-checks"
    }
  },
  "git-precommit-checks": {
    "rules": [
      {
        "message": "You’ve got leftover conflict markers",
        "regex": "/^[<>|=]{4,}/m"
      },
      {
        "filter": "\\.js$",
        "message": "You’ve got leftover `console.log`",
        "regex": "console\\.log"
      },
      {
        "filter": "\\.(js|ts)$",
        "message": "You have unfinished devs",
        "regex": "FIXME"
      }
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@reduxjs/toolkit": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "colorts": "^0.1.63",
    "dotenv-flow": "^3.2.0",
    "isomorphic-unfetch": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.3",
    "next": "10.0.3",
    "next-connect": "^0.9.1",
    "next-redux-wrapper": "^6.0.2",
    "react": "17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "17.0.1",
    "react-paypal-button-v2": "^2.6.2",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv-flow": "^3.1.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.2",
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/node": "^14.14.6",
    "@types/react": "^17.0.0",
    "@types/react-bootstrap": "^0.32.25",
    "@types/react-redux": "^7.1.11",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-editorconfig": "^2.1.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "git-precommit-checks": "^3.0.6",
    "husky": "^4.3.1",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}
